{
    "manifest_version" : 3,
    "name" : "Watch Wolf",
    "version" : "1.0",
    "description" : "Watch wolf helps to understand sentiment of text, pdf, images and tweets",
    "icons" : {
        "128" : "img/icons128.png",
        "48" : "img/icons48.png",
        "16" : "img/icons16.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128" : "img/icons128.png",
            "48" : "img/icons48.png",
            "16" : "img/icons16.png"
        }
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://twitter.com/",
        "https://watch-wolf.vercel.app/",
        "http://13.233.148.83/"
      ],
      "optional_host_permissions": [
        "*://*/*"
      ]
}